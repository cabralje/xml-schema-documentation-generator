    </xs:annotation>
  </xs:element>
  <xs:element name="ToolUser" type="nc:PersonType">
    <xs:annotation>
      <xs:documentation>A person who used the item as a tool.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="VictimEntityAbstract" abstract="true">
