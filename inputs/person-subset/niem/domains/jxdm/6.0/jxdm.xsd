      <xs:documentation>A person that embalmed the body of the decedent.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PersonPrimaryWorker" type="nc:PersonType">
    <xs:annotation>
      <xs:documentation>A primary social worker or other overseer assigned to a person that is not a caregiver.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PersonRaceCode" type="ncic:RACECodeType" substitutionGroup="nc:PersonRaceAbstract" nillable="true">
    <xs:annotation>
      <xs:documentation>A classification of a person based on factors such as geographical locations and genetics.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PersonRaceNDExCode" type="ndex:RACCodeType" substitutionGroup="nc:PersonRaceAbstract" nillable="true">
    <xs:annotation>
      <xs:documentation>A classification of a person based on factors such as geographical locations and genetics.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PersonRaceRapSheetCode" type="j:PersonRaceCodeType" substitutionGroup="nc:PersonRaceAbstract" nillable="true">
    <xs:annotation>
      <xs:documentation>A code set identifying a race of a person.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PersonReferralWorker" type="nc:PersonType">
    <xs:annotation>
      <xs:documentation>An Employee of State assigned to the particular juvenile.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ProgramViolationRelatedOfficial" type="nc:PersonType">
    <xs:annotation>
      <xs:documentation>An individual who observed or documented the program violation as a concatenated first and last name.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ProtectionOrderRestrictedPerson" type="nc:PersonType">
    <xs:annotation>
      <xs:documentation>A person that a subject is restricted from having any contact with as defined in a protection order.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ReceiverPerson" type="nc:PersonType">
